/* Global Styles */

html, body { height:100%; }
body { padding: 0px; font-size: 15px; }

a { color: #d64600; }
a:hover { color: #f89828; text-decoration: none; }

ul { list-style: none; padding: 6px 0px 4px 0px; margin: 0px 0px 12px 0px; }
ul li { position: relative; background: url('../../assets/images/general/Bullet.png') no-repeat 0px 6px;  border: none; min-height: 0px; width: auto; padding: 0px 10px 6px 20px; }

P { margin: 0 0 20px; }

hr { clear: both; border-top: 2px solid #e5e5e5; margin-top: 45px; margin-bottom: 45px;}

.clear-fix { clear: both; }

.float-left { float: left; }
.float-right { float: right; }

.add-bottom { margin-bottom: 35px; }

.rounded-corners-med { border-radius: 10px; -moz-border-radius: 10px; -webkit-border-radius: 10px; }

/* Headings */
h1, h2, h3, h4, h5, h6 { font-family: 'MontserratBold', Arial, Verdana, Helvetica; font-weight: 400; font-style: normal; -webkit-font-smoothing: antialiased; text-transform: uppercase; }

/* Colors */
.bg-orange-xlight { background: #f4e8f7; }
.bg-orange-light { background: #ffb660; }
.bg-orange-mid { background: #f89828; }

.bg-grey-light { background: #f1f1f1; }
.bg-grey-mid { background: #e3e3e3; }

.bg-black { background: #000000; border-radius: 15px; padding-top:10px;}

.orange { color: #f89828; }
.white { color: #fff; }


/* Page Sections */
.section.buy, .section.hire, .section.intro { padding: 0; }

.section.intro .padding { padding-top: 40px; padding-bottom: 40px; }
.section.hire .padding { padding-top: 30px; padding-bottom: 30px; }
.section.buy .padding { padding-top: 30px; padding-bottom: 30px; }
.section.product .padding { padding-top: 0px; padding-bottom: 20px; }
.section.general .padding { padding-top: 0px; padding-bottom: 20px; }
.section.product-detail .padding { padding-top: 40px; padding-bottom: 40px; }
.section.product-detail .title { padding-top: 0px; padding-bottom: 0px; padding-left: 0; }

.blog-posts p{
	margin-bottom:0;
}

/* Global Header */
.global-header { padding: 20px 0px 10px 0px; }

.global-header .sites { margin: 0px 0px 0px 0px; padding: 0px 0px 0px 0px; line-height: 1em; font-size: 13px;}
.global-header .sites li { float: right; border-right: 2px solid #cccccc; font-family: 'MontserratBold', Arial, Verdana, Helvetica; font-weight: 400; font-style: normal; -webkit-font-smoothing: antialiased; text-transform: uppercase; background: none; padding: 2px 10px 0px 10px; }
@media all and (-ms-high-contrast: none), (-ms-high-contrast: active) {
	.global-header .sites li { padding: 1px 10px 0px 10px; }
}
.global-header .sites .last-international-link {
	border:none;
}

.global-header .sites li a { color: #262626; }
.global-header .sites li a:hover { color: #f89828; text-decoration: none; }
.global-header .sites li.language { color: #949494; border: none; }
.global-header .sites li.language img { padding-left: 5px; }

.global-header .share-icons { margin-top: 10px; background: #efefef !important; border-radius: 4px; -moz-border-radius: 4px; -webkit-border-radius: 4px; padding: 4px 10px 4px 5px; }
.global-header .share-icons [class^="icon-"], .global-header .share-icons [class*=" icon-"] { float: left; margin-right: 3px; width: auto; font-size: 25px; }
.global-header .share-icons .icon-share { color: #999999; }

.global-header .share-icons .icon-facebook { color: #4364a2; }
.global-header .share-icons a:hover .icon-facebook { color: #1d3f7e; text-decoration: none; }

.global-header .share-icons .icon-twitter { color: #2ab1e3; }
.global-header .share-icons a:hover .icon-twitter { color: #1f9ac7; text-decoration: none; }

.global-header .share-icons .icon-googleplus { color: #de0000; }
.global-header .share-icons a:hover .icon-googleplus { color: #c30101; text-decoration: none; }

.global-header .share-icons .icon-comment { float: left; background: #d64600; border-radius: 12px; -moz-border-radius: 12px; -webkit-border-radius: 12px; width: 24px; height: 24px; line-height: 1.4em; font-size: 15px; color: #fff; padding: 3px;}
.global-header .share-icons a { color: #4d4d4d; }
.global-header .share-icons a:hover { color: #191919; text-decoration: none; }
.global-header .share-icons a:hover .icon-comment { background: #bc3d00; text-decoration: none; }

.global-header .share-icons .our-blog { float: left; }

.global-header .btn { padding: 3px 15px 1px 15px !important; font-size: 17px !important; margin-top: 10px; }

.global-header .contact-details { padding: 0; margin: 0; font-family: 'MontserratBold', Arial, Verdana, Helvetica; font-weight: 400; font-style: normal; -webkit-font-smoothing: antialiased; text-transform: uppercase; }
.global-header .contact-details li { float: right; background: none; }
.global-header .contact-details li.phone { font-size: 28px; padding-top: 8px; }
.global-header .contact-details li.phone a { color: #212121; }
.global-header .contact-details li.phone a { color: #000; text-decoration: none; }

.global-header .contact-details li.phone .icon-phone { font-size: 23px; color:#b2b2b2; }
.global-header .contact-details li.phone a .icon-phone { color:#b2b2b2; }
.global-header .contact-details li.phone a:hover .icon-phone { color:#585858; text-decoration: none; }


/* Global Navigation */
.navbar { margin-bottom: 0; font-family: 'MontserratBold', Arial, Verdana, Helvetica; font-weight: 400; font-style: normal; -webkit-font-smoothing: antialiased; text-transform: uppercase; color: #fff; padding: 0; border-radius: 0; }
.navbar-nav { padding: 0; }
.nav>li { background: none; padding: 0; }
.nav>li>a { color: #fff !important; border-right: 1px solid #3c3c3c; }
.nav>li>a:hover { color: #f89828 !important; }
.nav li:nth-last-child(2) a { border-right: none; }

.dropdown-menu { background: #222; }
.ie8 .dropdown-menu { border: 1px solid #222; }
.dropdown-menu>li>a { color: #fff; }
.dropdown-menu>li { background: none; padding: 4px 0px 4px 0px; }
.dropdown-menu>li>a:hover, .dropdown-menu>li>a:focus { background: #363636; color: #f89828; }


/* Booking Form */
.booking-form { background: #f89828; border-radius:10px;}
.booking-form form { position: relative; margin: 0 auto; margin-top: 20px; width: 100%;}

.booking-form .heading { background: #262626; border-top-left-radius:10px; border-top-right-radius: 10px; color: #fff; font-family: 'MontserratBold', Arial, Verdana, Helvetica; font-weight: 400; font-style: normal; -webkit-font-smoothing: antialiased; text-transform: uppercase; font-size: 26px; padding: 20px 20px 13px 20px; line-height: 0.8em; }
.booking-form .heading .small { font-size: 15px; }

.booking-form select {padding: 3px;font-size: 15px;background: #fff;height: 31px;}
.booking-form input[type="text"] { border: 1px solid #a7a7a7; border-radius: 4px; -moz-border-radius: 4px; -webkit-border-radius: 4px; padding: 3px; }

.booking-form .step1, .booking-form .step2, .booking-form .step3, .booking-form .step4, .booking-form .step5 { position: relative; margin: 0 auto; width: 87%; margin-bottom: 15px; }
.booking-form .step1 label, .booking-form .step2 label, .booking-form .step3 label, .booking-form .step4 label, .booking-form .step5 label { font-family: 'MontserratBold', Arial, Verdana, Helvetica; font-weight: 400; font-style: normal; -webkit-font-smoothing: antialiased; font-size: 16px !important; margin-bottom: 0; width: 100%; text-transform:none;}

.booking-form .step1 select { width: 100%; }
.booking-form .step2 select, .booking-form .step3 select { width: 53%; }
.booking-form .step2 input[type="text"], .booking-form .step3 input[type="text"] {width: 35%;margin-right: 6px;}
.booking-form .step4 input[type="text"],
.booking-form .step5 select {width: 100% !important;}

.booking-form .icon-calendar { background: #d64600; padding: 5px; border-radius: 22px; -moz-border-radius: 22px; -webkit-border-radius: 22px; width: 44px; height: 44px; font-size: 21px; color: #f89828; text-align: center; }
.booking-form input[type="submit"] { 
	width: 100%; 
	background: #d64600; 
	border-bottom-left-radius:10px; 
	border-bottom-right-radius: 10px; 
	border: none; 
	font-family: 'MontserratBold', Arial, Verdana, Helvetica; 
	font-weight: 400; 
	font-style: normal; 
	-webkit-font-smoothing: antialiased; 
	text-transform: uppercase; 
	color: #fff; 
	font-size: 21px; 
	padding: 10px 0px 10px 40px; 
	margin-top: 10px;
	background-image:url('../images/general/get-quote-arrow.png');
	background-color:#d64600;
	background-repeat:no-repeat;
	background-position:right center;
	text-align:left;
}


	.booking-form .heading .small{
		text-transform:none;
	}
	



/* Booking Form - Homepage */
.container.relative { position: relative; left: 0; }
.booking-container { position: absolute; overflow: hidden; z-index: 45; left: 0; border-radius:15px;}
.booking-container .booking-form { position: relative; z-index: 35; width: 340px; left: 0%; top: 6%; }

/* Forms */
.enquiryform select { padding: 3px; font-size: 15px; background: #fff; height: 28px; }
.enquiryform input[type="text"], .enquiryform input[type="email"] { border: 1px solid #a7a7a7; border-radius: 4px; -moz-border-radius: 4px; -webkit-border-radius: 4px; padding: 3px; }
.enquiryform textarea { border: 1px solid #a7a7a7; border-radius: 4px; -moz-border-radius: 4px; -webkit-border-radius: 4px; padding: 3px; }
.enquiryform .gform_wrapper .gsection { border-bottom: 4px solid #ccc; }


/* Banners */
.banner .text { position:absolute; overflow: hidden; z-index: 15; text-align:left; /* background-color: rgba(0,0,0,0.45); */ padding:20px;}
.banner .text .main-heading { color: #fff; font-family: 'MontserratBold', Arial, Verdana, Helvetica; font-weight: 400; font-style: normal; -webkit-font-smoothing: antialiased; text-transform: uppercase;}

.ie8 .banner .item { position:relative; }

.owl-carousel { background: #ececdc; z-index: 1 }
.owl-theme .owl-controls { position: absolute; bottom: 2.5%; text-align: center !important; }
.owl-theme .owl-controls .owl-page span { width: 18px !important; height: 18px !important; margin: 5px 4px !important; background: #fff !important; opacity: 1 !important;
-webkit-box-shadow: 0 0px 4px rgba(0, 0, 0, .80);
box-shadow: 0 0px 4px rgba(0, 0, 0, .80); 
}
.owl-theme .owl-controls .owl-page.active span { background: #f89828 !important; }




/* ------------------------------- Homepage ------------------------------- */

/* Headings - Homepage */
.intro h1 { font-size: 35px; color: #000; margin: 0px 0px 2px 0px;  }
.intro h2 { font-size: 30px; color: #f89828; margin: 0px 0px 20px 0px; padding: 0; }

